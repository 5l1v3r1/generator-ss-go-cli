{
  "name": "generator-ss-go-cli",
  "version": "1.0.0",
  "description": "yeoman generator to develop cli tool with Golang",
  "homepage": "https://github.com/suzuki-shunsuke/generator-ss-go-cli",
  "author": {
    "name": "Suzuki Shunsuke",
    "email": "suzuki.shunsuke.1989@gmail.com",
    "url": "https://github.com/suzuki-shunsuke"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["yeoman-generator", "go", "cli"],
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-xo": "0.18.2",
    "eslint-plugin-prettier": "2.7.0",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "lint-staged": "4.3.0",
    "nsp": "2.8.1",
    "prettier": "1.9.2",
    "standard-version": "4.3.0",
    "validate-commit-msg": "2.14.0",
    "yeoman-assert": "3.1.0",
    "yeoman-test": "1.7.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "release": "standard-version",
    "commitmsg": "validate-commit-msg"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "suzuki-shunsuke/generator-ss-go-cli",
  "license": "MIT"
}
